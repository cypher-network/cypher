{
  "Api": {
    "Listening": "http://0.0.0.0:7000",
    "Advertise": "http://0.0.0.0:7000"
  },
  "DataFolder": "storedb",
  "DataProtectionPath": "keys",
  "Log": {
    "FirstChanceException": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Enrich": "FromLogContext",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] [{MemberName}:{LineNumber}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "path": "cypnode.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "SeedNodes": [],
  "Serf": {
    "Advertise": "",
    "Listening": "0.0.0.0:7946",
    "RPC": "127.0.0.1:7373",
    "Encrypt": "",
    "SnapshotPath": "",
    "NodeName": "cypher",
    "RetryMax": 5,
    "Rejoin": true,
    "BroadcastTimeout": "30s",
    "Loglevel": "debug",
    "Profile": "wan",
    "Replay": false,
    "KeyringFile": "",
    "Disabled": false
  },
  "Staking": {
    "OnOff": false,
    "BlockTransactionCount": 100,
    "WalletSettings": {
      "Identifier": "",
      "Passphrase": "",
      "Address": "",
      "Url": "http://localhost:8001",
      "SendPaymentEndpoint": "/api/wallet/transaction"
    }
  }
}
